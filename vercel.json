{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
        "src": "/list/(.*)",
        "dest": "src/main.ts",
        "methods": [
            "GET",
            "POST",
            "PATCH",
            "DELETE"
        }
        ]
    ]
}